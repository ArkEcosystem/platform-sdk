#!/usr/bin/env node

const {
	createProfileWithWallets,
	openProfile,
	transferAda,
	transferArk,
	transferTrx,
	transferXlm,
	signMessageXlm,
} = require("../dist");

const examples = {
	"create-profile": createProfileWithWallets,
	"open-profile": openProfile,
	"transfer-ada": transferAda,
	"transfer-ark": transferArk,
	"transfer-trx": transferTrx,
	"transfer-xlm": transferXlm,
	"sign-message-xlm": signMessageXlm,
};

const { input } = require("./meow");

examples[input[0]]();

