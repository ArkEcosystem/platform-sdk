#!/usr/bin/env node

const {
	createProfileWithWallets,
	openProfile,
	transferAda,
	transferArk,
	transferTrx,
} = require("../dist");

const { input } = require("./meow");

if (input[0] === "create-profile") {
	(async () => createProfileWithWallets())();
}

if (input[0] === "open-profile") {
	(async () => openProfile())();
}

if (input[0] === "transfer-ada") {
	(async () => transferAda())();
}

if (input[0] === "transfer-ark") {
	(async () => transferArk())();
}

if (input[0] === "transfer-trx") {
	(async () => transferTrx())();
}
